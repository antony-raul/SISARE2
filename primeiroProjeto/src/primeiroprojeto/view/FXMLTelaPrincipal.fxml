<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="primeiroprojeto.controller.FXMLTelaPrincipalController">
   <children>
      <TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Inicio">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
          <Tab text="Alunos">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="311.0" prefWidth="570.0">
                     <children>
                        <TableView fx:id="tableView" layoutX="10.0" layoutY="24.0" prefHeight="295.0" prefWidth="578.0">
                          <columns>
                            <TableColumn fx:id="nomeCol" prefWidth="177.0" text="Nome" />
                            <TableColumn fx:id="matriculaCol" prefWidth="85.0" text="Matricula" />
                            <TableColumn fx:id="ruaCol" prefWidth="106.0" text="Rua" />
                            <TableColumn fx:id="numeroCol" prefWidth="54.0" text="Número" />
                            <TableColumn fx:id="cursoCol" prefWidth="87.0" text="Curso" />
                            <TableColumn fx:id="statusCol" prefWidth="68.0" text="Status" />
                          </columns>
                        </TableView>
                        <Button fx:id="btnCreate"  onAction="#handleJanelaCadastrarAluno" layoutX="307.0" layoutY="331.0" mnemonicParsing="false" prefHeight="0.0" prefWidth="79.0" styleClass="Button" stylesheets="@default.css" text="Casdatrar" textFill="#fffefe">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                        <Button onAction="#handleJanelaEditarAluno" layoutX="406.0" layoutY="331.0" mnemonicParsing="false" prefHeight="0.0" prefWidth="79.0" styleClass="Button" stylesheets="@default.css" text="Editar" textFill="#fffefe">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                        <Button onAction="#bloquearAluno" layoutX="502.0" layoutY="331.0" mnemonicParsing="false" prefHeight="0.0" prefWidth="79.0" styleClass="Button" stylesheets="@default.css" text="Bloquear" textFill="#fffefe">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab text="Materiais">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView layoutX="9.0" layoutY="29.0" prefHeight="290.0" prefWidth="582.0">
                          <columns>
                            <TableColumn prefWidth="75.0" text="Nome" />
                            <TableColumn prefWidth="75.0" text="Quantidade" />
                              <TableColumn prefWidth="75.0" text="Tipo" />
                          </columns>
                        </TableView>
                        <Button layoutX="300.0" layoutY="332.0" mnemonicParsing="false" prefHeight="0.0" prefWidth="79.0" styleClass="Button" stylesheets="@default.css" text="Casdatrar" textFill="#fffefe">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                        <Button layoutX="399.0" layoutY="332.0" mnemonicParsing="false" prefHeight="0.0" prefWidth="79.0" styleClass="Button" stylesheets="@default.css" text="Editar" textFill="#fffefe">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                        <Button layoutX="495.0" layoutY="332.0" mnemonicParsing="false" prefHeight="0.0" prefWidth="79.0" styleClass="Button" stylesheets="@default.css" text="Bloquear" textFill="#fffefe">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
            <Tab text="Espaços">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
